{
    "name": "@openharmony/faceauth",
    "version": "3.1.0",
    "description": "Face authentication capability",
    "homePage": "https://gitee.com/openharmony",
    "author": {},
    "repository": "",
    "license": "Apache License 2.0",
    "publishAs": "code-segment",
    "segment": {
